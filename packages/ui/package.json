{
  "name": "@app-ds/ui",
  "version": "0.0.0",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./client": "./dist/components/client.js",
    "./server": "./dist/components/server.js",
    "./index.css": "./dist/index.css"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias && tailwindcss -i ./src/styles/styles.css -o dist/index.css && copy-and-watch -r ./assets/fonts/**/* ./dist/fonts/",
    "dev": " concurrently \"tsc -p tsconfig.json -w\" \"tsc-alias -w\" \"tailwindcss --watch -i ./src/styles/styles.css -o ./dist/index.css\" \"copy-and-watch --watch -r ./assets/fonts/**/* ./dist/fonts/\"",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "copy-and-watch": "^0.1.6",
    "eslint": "^8.55.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
